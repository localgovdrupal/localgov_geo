{
    "name": "localgovdrupal/localgov_geo",
    "type": "drupal-module",
    "description": "LocalGov Drupal Geo integration",
    "homepage": "https://github.com/localgovdrupal/localgov_geo",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal/geofield": "^1.15",
        "drupal/geofield_map": "^2.66",
        "drupal/geocoder": "^3.7",
        "drupal/address": "^1.8",
        "drupal/leaflet": "^2.0",
        "geocoder-php/nominatim-provider": "^5.2"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/leaflet": {
                "Fix schema #2883700": "https://www.drupal.org/files/issues/2020-07-31/leaflet-schema-288370-6.patch"
            }
        }
    }
}
